a\",\"lhs\":{\"uuid\":\"d5cd6930-3550-11f0-bf93-f92af100181a\",\"parent_uuid\":\"d3564eb0-3550-11f0-bf93-f92af100181a\",\"value\":\"runCode.enableProgressBar\",\"comparison_value_type\":\"EnvironmentValue\",\"full_environment_path\":\"runCode.enableProgressBar\"},\"rhs\":{\"uuid\":\"d6427220-3550-11f0-bf93-f92af100181a\",\"parent_uuid\":\"d3564eb0-3550-11f0-bf93-f92af100181a\",\"value\":\"true\",\"comparison_value_type\":\"LiteralValue\"},\"value_type\":\"EnvironmentScalarDefinition:Boolean\",\"operator\":\"==\",\"operation_type\":\"Comparison\"},\"operator\":\"AND\",\"operation_type\":\"LogicalExpression\"}"}],"task_id":"shopify::flow::condition","task_version":"0.1","task_type":"CONDITION","description":null,"note":null,"name":null},{"step_id":"88fa6620-3551-11f0-bf93-f92af100181a","step_position":[1080,120],"config_field_values":[{"config_field_id":"input","value":"query {\n  shop {\n    metafields {\n      namespace\n      key\n      value\n    }\n  }\n  order {\n    totalPriceSet {\n      shopMoney {\n        amount\n      }\n    }\n  }\n}\n"},{"config_field_id":"script","value":"export default function main(input) {\n  const mf = (input.shop?.metafields || []).find(m => m.namespace === \"global\" && m.key === \"tier_data\");\n  const orderTotal = parseFloat(input.order?.totalPriceSet?.shopMoney?.amount || 0);\n  if (!mf) return { eligibleForGift: false };\n\n  try {\n    const data = JSON.parse(mf.value);\n    if (!data.enable_progress_bar || !Array.isArray(data.tier) || data.tier.length === 0) return { eligibleForGift: false };\n    const minThreshold = Math.min(...data.tier.map(t => t.threshold || Infinity));\n    return { eligibleForGift: orderTotal >= minThreshold };\n  } catch {\n    return { eligibleForGift: false };\n  }\n}\n"},{"config_field_id":"output_schema","value":"type Output {\n  eligibleForGift: Boolean!\n}\n"}],"task_id":"shopify::flow::run_code","task_version":"0.1","task_type":"ACTION","description":null,"note":null,"name":"Run code (1)"},{"step_id":"9b570580-3551-11f0-bf93-f92af100181a","step_position":[1440,100],"config_field_values":[{"config_field_id":"condition","value":"{\"uuid\":\"9edd1f02-3551-11f0-bf93-f92af100181a\",\"lhs\":{\"uuid\":\"9d2de860-3551-11f0-bf93-f92af100181a\",\"parent_uuid\":\"9edd1f02-3551-11f0-bf93-f92af100181a\",\"lhs\":{\"uuid\":\"9edd1f00-3551-11f0-bf93-f92af100181a\",\"parent_uuid\":\"9d2de860-3551-11f0-bf93-f92af100181a\",\"value\":\"runCode1.eligibleForGift\",\"comparison_value_type\":\"EnvironmentValue\",\"full_environment_path\":\"runCode1.eligibleForGift\"},\"rhs\":{\"uuid\":\"9fa15960-3551-11f0-bf93-f92af100181a\",\"parent_uuid\":\"9d2de860-3551-11f0-bf93-f92af100181a\",\"value\":\"true\",\"comparison_value_type\":\"LiteralValue\"},\"value_type\":\"EnvironmentScalarDefinition:Boolean\",\"operator\":\"==\",\"operation_type\":\"Comparison\"},\"operator\":\"AND\",\"operation_type\":\"LogicalExpression\"}"}],"task_id":"shopify::flow::condition","task_version":"0.1","task_type":"CONDITION","description":null,"note":null,"name":null},{"step_id":"ca7d0760-3551-11f0-bf93-f92af100181a","step_position":[1980,120],"config_field_values":[{"config_field_id":"condition","value":"{\"uuid\":\"cef2ff20-3551-11f0-bf93-f92af100181a\",\"lhs\":{\"uuid\":\"cc2671a0-3551-11f0-bf93-f92af100181a\",\"parent_uuid\":\"cef2ff20-3551-11f0-bf93-f92af100181a\",\"lhs\":{\"uuid\":\"cef2d810-3551-11f0-bf93-f92af100181a\",\"parent_uuid\":\"cc2671a0-3551-11f0-bf93-f92af100181a\",\"value\":\"runCode.tierType\",\"comparison_value_type\":\"EnvironmentValue\",\"full_environment_path\":\"runCode.tierType\"},\"rhs\":{\"uuid\":\"d3afeaf0-3551-11f0-bf93-f92af100181a\",\"parent_uuid\":\"cc2671a0-3551-11f0-bf93-f92af100181a\",\"value\":\"add\",\"comparison_value_type\":\"LiteralValue\"},\"value_type\":\"EnvironmentScalarDefinition:String\",\"operator\":\"==\",\"operation_type\":\"Comparison\"},\"operator\":\"AND\",\"operation_type\":\"LogicalExpression\"}"}],"task_id":"shopify::flow::condition","task_version":"0.1","task_type":"CONDITION","description":null,"note":null,"name":null},{"step_id":"e6dfdfe0-3551-11f0-bf93-f92af100181a","step_position":[2340,120],"config_field_values":[{"config_field_id":"input","value":"query {\n  shop {\n    metafields {\n      namespace\n      key\n      value\n    }\n  }\n  order {\n    totalPriceSet {\n      shopMoney {\n        amount\n      }\n    }\n    lineItems {\n      variant {\n        id\n      }\n    }\n  }\n}\n"},{"config_field_id":"script","value":"export default function main(input) {\n  const orderTotal = Number(input.order?.totalPriceSet?.shopMoney?.amount) || 0;\n  const orderVariantIds = new Set(\n    (input.order?.lineItems || [])\n      .map(item => item.variant?.id?.split('/').pop())\n      .filter(Boolean)\n  );\n  const metafields = input.shop?.metafields || [];\n\n  // Find tier_data metafield in global namespace\n  const tierMetafield = metafields.find(mf => mf.namespace === \"global\" && mf.key === \"tier_data\");\n  if (!tierMetafield?.value) return generateEmptyOutput(0);\n\n  let tierData;\n  try {\n    tierData = JSON.parse(tierMetafield.value);\n  } catch {\n    return generateEmptyOutput(0);\n  }\n\n  const tiers = tierData?.tier;\n  if (!Array.isArray(tiers) || tiers.length === 0) return generateEmptyOutput(0);\n\n  // Filter eligible tiers where orderTotal >= threshold, sorted ascending\n  const eligibleTiers = tiers.filter(t => orderTotal >= t.threshold).sort((a, b) => a.threshold - b.threshold);\n\n  const maxTiers = tiers.length;\n  const output = {};\n\n  for (let i = 0; i < maxTiers; i++) {\n    if (i < eligibleTiers.length) {\n      const variantId = String(eligibleTiers[i].variant_id);\n      const isMissing = !orderVariantIds.has(variantId);\n      output[`variant${i + 1}Missing`] = isMissing;\n      output[`variant${i + 1}GID`] = `gid://shopify/ProductVariant/${variantId}`;\n    } else {\n      output[`variant${i + 1}Missing`] = false;\n      output[`variant${i + 1}GID`] = \"\";\n    }\n  }\n\n  return output;\n\n  function generateEmptyOutput(count) {\n    const emptyOutput = {};\n    for (let i = 0; i < count; i++) {\n      emptyOutput[`variant${i + 1}Missing`] = false;\n      emptyOutput[`variant${i + 1}GID`] = \"\";\n    }\n    return emptyOutput;\n  }\n}\n"},{"config_field_id":"output_schema","value":"type Output {\n  variant1Missing: Boolean!\n  variant1GID: String!\n\n  variant2Missing: Boolean!\n  variant2GID: String!\n\n  variant3Missing: Boolean!\n  variant3GID: String!\n\n}\n"}],"task_id":"shopify::flow::run_code","task_version":"0.1","task_type":"ACTION","description":null,"note":null,"name":"Run code (2)"},{"step_id":"385b06e0-3554-11f0-8681-1158edeccd52","step_position":[1980,280],"config_field_values":[{"config_field_id":"condition","value":"{\"uuid\":\"3e4241e2-3554-11f0-8681-1158edeccd52\",\"lhs\":{\"uuid\":\"3a9cb980-3554-11f0-8681-1158edeccd52\",\"parent_uuid\":\"3e4241e2-3554-11f0-8681-1158edeccd52\",\"lhs\":{\"uuid\":\"3e4241e0-3554-11f0-8681-1158edeccd52\",\"parent_uuid\":\"3a9cb980-3554-11f0-8681-1158edeccd52\",\"value\":\"runCode.tierType\",\"comparison_value_type\":\"EnvironmentValue\",\"full_environment_path\":\"runCode.tierType\"},\"rhs\":{\"uuid\":\"405a4c70-3554-11f0-8681-1158edeccd52\",\"parent_uuid\":\"3a9cb980-3554-11f0-8681-1158edeccd52\",\"value\":\"replace\",\"comparison_value_type\":\"LiteralValue\"},\"value_type\":\"EnvironmentScalarDefinition:String\",\"operator\":\"==\",\"operation_type\":\"Comparison\"},\"operator\":\"AND\",\"operation_type\":\"LogicalExpression\"}"}],"task_id":"shopify::flow::condition","task_version":"0.1","task_type":"CONDITION","description":null,"note":null,"name":null},{"step_id":"712c2990-3554-11f0-8681-1158edeccd52","step_position":[2340,280],"config_field_values":[{"config_field_id":"input","value":"query {\n  shop {\n    metafields {\n      namespace\n      key\n      value\n    }\n  }\n  order {\n    lineItems {\n      variant {\n        id\n      }\n    }\n    totalPriceSet {\n      shopMoney {\n        amount\n      }\n    }\n  }\n}\n"},{"config_field_id":"script","value":"export default function main(input) {\n  const orderTotal = Number(input.order?.totalPriceSet?.shopMoney?.amount) || 0;\n  const orderVariantIds = new Set(\n    (input.order?.lineItems || []).map(item => item.variant?.id?.split('/').pop()).filter(Boolean)\n  );\n  const metafields = input.shop?.metafields || [];\n\n  const tierMetafield = metafields.find(mf => mf.namespace === \"global\" && mf.key === \"tier_data\");\n  if (!tierMetafield?.value) return { variantMissing: false, variantGID: \"\" };\n\n  let tierData;\n  try {\n    tierData = JSON.parse(tierMetafield.value);\n  } catch {\n    return { variantMissing: false, variantGID: \"\" };\n  }\n\n  if (!Array.isArray(tierData.tier) || tierData.tier.length === 0) return { variantMissing: false, variantGID: \"\" };\n\n  // Find highest threshold tier where orderTotal >= threshold\n  const eligibleTiers = tierData.tier.filter(t => orderTotal >= t.threshold);\n  if (eligibleTiers.length === 0) return { variantMissing: false, variantGID: \"\" };\n\n  const highestTier = eligibleTiers.reduce((max, t) => (t.threshold > max.threshold ? t : max), eligibleTiers[0]);\n  const variantId = String(highestTier.variant_id);\n  const variantMissing = !orderVariantIds.has(variantId);\n\n  return {\n    variantMissing,\n    variantGID: variantMissing ? `gid://shopify/ProductVariant/${variantId}` : \"\",\n  };\n}\n"},{"config_field_id":"output_schema","value":"type Output {\n  variantMissing: Boolean!\n  variantGID: String!\n}\n"}],"task_id":"shopify::flow::run_code","task_version":"0.1","task_type":"ACTION","description":null,"note":null,"name":"Run code (3)"},{"step_id":"98d7f460-3554-11f0-8681-1158edeccd52","step_position":[2700,280],"config_field_values":[{"config_field_id":"condition","value":"{\"uuid\":\"9e2ab790-3554-11f0-8681-1158edeccd52\",\"lhs\":{\"uuid\":\"9c38f9b0-3554-11f0-8681-1158edeccd52\",\"parent_uuid\":\"9e2ab790-3554-11f0-8681-11